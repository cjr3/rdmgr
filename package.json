{
  "name": "rdmgr",
  "version": "0.2.3",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.28",
    "babel-plugin-root-import": "^6.2.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "electron-is-dev": "^1.1.0",
    "electron-packager": "^14.2.1",
    "http": "0.0.0",
    "material-ui-color-picker": "^3.4.0",
    "node-sass": "^4.13.1",
    "peer": "^0.2.10",
    "peerjs": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "socket.io": "^2.3.0",
    "socket.io-stream": "^0.9.1",
    "tiny-worker": "^2.3.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.2",
    "wait-on": "^3.3.0",
    "write-file-atomic": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "build",
    "electron-pack": "npm run build -w",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3001 && electron .\"",
    "build-electron": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package": "electron-builder build --win --publish never"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "Carl Roberts",
    "email": "cjr3@live.com",
    "url": "http://carljroberts.com"
  },
  "description": "Banked Track Roller Derby A/V Production Software",
  "devDependencies": {
    "electron": "^6.1.7",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "com.carljroberts.rdmgr",
    "productName": "RDMGR",
    "copyright": "Copyright 2017-2020 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    }
  }
}
